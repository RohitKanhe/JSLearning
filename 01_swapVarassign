//Swapping Names

console.log("Swapping Names");
var name1="sweety"; //cutie
var name2="cutie"; //sweety

var temp= name1;
name1=name2;
name2=temp;
console.log(name1 , name2);


//Swapping numbers

console.log("Swapping numbers");
var num1=100; //200
var num2=200; //300
var num3=300; //100

var temp= num1;
num1=num2;
num2=num3;
num3=temp;
console.log(num1 , num2, num3);